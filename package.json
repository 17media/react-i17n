{
  "name": "react-i17n",
  "version": "0.2.0",
  "description": "A react-intl alternative i18n library for react 16",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "build-locale": "node scripts/build-locale",
    "postbuild": "npm run test:bundlesize",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:bundlesize": "bundlesize",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "prepare": "npm run build-locale && npm run build"
  },
  "dependencies": {
    "intl-format-cache": "^2.1.0",
    "intl-messageformat": "^2.2.0",
    "intl-relativeformat": "^2.1.0",
    "prop-types": "^15.6.0",
    "react-broadcast": "0.7.0-rc.4"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "bundlesize": "~0.16.0",
    "formatjs-extract-cldr-data": "4.1.0",
    "mkdirp": "~0.5.1",
    "nwb": "0.21.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "~2.6.2",
    "serialize-javascript": "~1.4.0"
  },
  "author": "Kai Hao",
  "homepage": "https://github.com/17media/react-i17n",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react 16",
    "react-intl",
    "intl",
    "i18n"
  ],
  "bundlesize": [
    {
      "path": "./umd/react-i17n.min.js",
      "maxSize": "15 kB"
    }
  ]
}
